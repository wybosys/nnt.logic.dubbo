{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "config": {
    "sidexpire": 86400,
    "cidexpire": 600,
    "cache": "cache",
    "https": false,
    "deskey": "joi>:90",
    "cluster": 1,
    "apollo": {
      "host": "http://apollo-config-server:8080",
      "appid": "test-logic",
      "key": "nnt.logic.jvm"
    }
  },
  "server": [
    {
      "id": "sample-app",
      "entry": "com.test.app.Test",
      "listen": "*:8090",
      "router": {
        "com.nnt.server.apidoc.Router": {
          "export": {
            "router": [
              "com.test.app.router.RSample"
            ],
            "model": [
              "com.test.app.model.Echoo"
            ]
          }
        }
      }
    },
    {
      "id": "sample-dubbo",
      "entry": "com.nnt.dubbo.Dubbo",
      "protocol": [
        {
          "type": "dubbo",
          "port": 8091,
          "service": [
            "test"
          ]
        },
        {
          "type": "rest",
          "port": 8092,
          "service": [
            "test"
          ]
        },
        {
          "type": "grpc",
          "port": 8093,
          "service": [
            "test",
            "test1"
          ]
        }
      ],
      "registry": {
        "type": "zookeeper",
        "host": "localhost:2181"
      },
      "service": [
        {
          "id": "test",
          "impl": "com.test.dubbo.Test",
          "iface": "com.test.dubbo.ITest"
        },
        {
          "id": "test1",
          "impl": "com.test.dubbo.Test1",
          "iface": "com.test.dubbo.ITest1"
        }
      ]
    },
    {
      "id": "sample-grpc-web",
      "entry": "com.nnt.server.GrpcWeb",
      "listen": "*:8094",
      "service": [
        "com.test.dubbo.Test",
        "com.test.dubbo.Test1"
      ]
    }
  ],
  "dbms": [
    {
      "id": "mysql",
      "host": "localhost:3306",
      "user": "root",
      "pwd": "root",
      "schema": "nnt-logic",
      "entry": "com.nnt.store.RMysql",
      "mybatis": {
        "map": [
          "bundle://mybatis/sample.xml"
        ]
      }
    }
  ],
  "logger": [
    {
      "id": "log4j",
      "filter": "all",
      "entry": "com.nnt.logger.Log4j",
      "dir": "cache",
      "name": "app",
      "pattern": "[%d{ISO8601}{TIMEZONE}] %p: %m%n",
      "roll": {
        "size": "100M",
        "daily": false
      }
    }
  ]
}
